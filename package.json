{
  "name": "aix",
  "version": "1.0.239",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "FEATURE_DOTENV=true nodemon --inspect -L --exec node -r esm packages/web/index.js",
    "dev": "FEATURE_DOTENV=true nodemon --inspect -L --exec node -r esm packages/web/index.js",
    "cli": "FEATURE_DOTENV=1 nodemon --inspect -q --no-stdin --exec node -r esm packages/cli/bin/run -t 576489306",
    "start:tunnel": "ngrok http localhost:5000",
    "start:services": "node -r esm ./node_modules/.bin/moleculer-runner -H -r --config dev.config.js --env services/*/*.service.js",
    "start:web": "DEBUG=telegram FEATURE_DOTENV=true node -r esm util/telegram.js",
    "start:mob": "NODEID=aix-mob moleculer-runner -r --env services/mob/mob.service.js",
    "start:chat": "NODEID=aix-chat moleculer-runner -r --env services/chat/chat.service.js",
    "debug": "node --inspect-brk -r esm  index.js",
    "fixtures": "node -r dotenv/config -r babel-core/register services/db/fixtures/fixtures-loader-script.js",
    "test": "DEBUG=-* MOB_USE_ORDER_PREFIX=true NODE_ENV=unit_test jest --coverage services/chat services/rfq services/quote services/mob services/sre services/quote services/session packages/web",
    "lint": "node_modules/.bin/eslint --ext=.js services packages",
    "lint:fix": "node_modules/.bin/eslint --fix --ext=.js services packages",
    "docker:login": "aws ecr get-login --no-include-email --region eu-central-1 | /bin/bash",
    "docker:run": "docker run -p 5000:5000 --env-file .env aix-next",
    "docker:build": "docker build -t aix-next .",
    "docker:tag": "docker tag aix-next:latest 258544608464.dkr.ecr.eu-central-1.amazonaws.com/aix-next:latest",
    "docker:push": "docker push 258544608464.dkr.ecr.eu-central-1.amazonaws.com/aix-next:latest",
    "validate": "npm-run-all --parallel lint",
    "precommit": "npm run validate",
    "prepush": "npm run validate",
    "jest": "cross-env MOB_USE_ORDER_PREFIX=true NODE_ENV=unit_test jest --coverage --coverageReporters=lcov --no-cache services/chat/",
    "api:documentation": "apidoc -i services/ -o services/api/apidoc/ -f /services/api/actions"
  },
  "keywords": [],
  "author": "alfonsodev <alfonso.fernandez@saltandpepper.co>",
  "license": "ISC",
