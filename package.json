    "delay": "^4.1.0",
    "dotenv-safe": "^6.0.0",
    "esm": "^3.0.49",
    "express": "^4.16.3",
    "jest-cli": "^23.6.0",
    "moleculer": "^0.13.3",
    "moleculer-web": "^0.8.0",
    "mqtt": "^2.18.1",
    "path": "^0.12.7",
    "readable-stream": "^3.0.3",
    "request": "^2.88.0",
    "request-promise": "^4.2.2",
    "socket.io-stream": "^0.9.1",
    "uuid": "^3.3.2"
  }
